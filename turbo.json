{
	"$schema": "https://turbo.build/schema.json",
	"globalEnv": ["NODE_ENV"],
	"pipeline": {
		"build": {
			"outputs": ["dist/**", ".next/**"],
			"dependsOn": ["^build"]
		},
		"generate": {
			"dependsOn": ["^generate"]
		},
		"db:migrate:deploy": {},
		"db:push": {},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
